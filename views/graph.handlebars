{{!-- {{This is the graph page handlebars}} --}}

<link rel="stylesheet" href="/public/main.css">


{{!-- {{Navigation bar}} --}}

<nav class="navbar navbar-expand-lg navbar-light bg-light">

  <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent"
    aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
    <span class="navbar-toggler-icon"></span>
  </button>

  <div class="collapse navbar-collapse">
    <ul class="navbar-nav mr-auto">
      <a class="nav-link" href="./">Home</a>

      <li class="nav-item active">
        <a class="nav-link" href="./graph">Reports</a>
      </li>
      <a class="nav-link" href="./settings">Settings</a>

    </ul>
  </div>
</nav>

<p> Graph Page</p>
<button id="graph-info">Graph Test</button>
{{#if data}}
<ul id="lmao" style="color:white;display:none">
  {{#each data}}
  <li>{{this.time}}, {{this.val}}</li>
  {{/each}}
</ul>
{{else}}
<p><em>There is currently no data being sent</em></p>
{{/if}}
<script>
  $(document).ready(function () {
    console.log($("#lmao").text())
  })
  $('#graph-info').click(function () {
    $.ajax({
      type: 'POST',
      url: "/graph/test",
      data: {},
      success: function (data) {
        console.log('success')
      },
      error: function (data, error, xhr) {
        console.log('error')
      },
      dataType: 'json'
    });
  });
</script>